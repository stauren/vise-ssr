{
  "name": "@vise-ssr/express-server",
  "version": "0.6.2",
  "description": "HTTP server based on express for vise-ssr",
  "type": "module",
  "bin": {
    "vise-express": "bin/vise-express.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "rollup -c ./rollup.config.js",
    "clean": "rimraf ./dist ./client",
    "lint": "eslint --ext .ts,.js,.vue ./src",
    "benchmark": "./bin/vise-express.js start -r 100 -c false ../app-vue3-intro",
    "serve": "rimraf ./client/client/*.html && cross-env NODE_ENV=production node ./dist/index.js start"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "commander": "^8.3.0",
    "cookie": "^0.4.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "pm2": "^5.2.0",
    "vise-ssr": "0.6.2",
    "zx": "^6.0.7"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "rollup": ">=2.59.0 <2.78.0",
    "eslint": "^7.32.0",
    "cross-env": "^7.0.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "rollup-plugin-executable": "^1.6.3",
    "rollup-plugin-preserve-shebangs": "^0.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "stauren@qq.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/stauren/vise-ssr.git",
    "directory": "packages/express-server"
  },
  "license": "MIT"
}
