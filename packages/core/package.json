{
  "name": "vise-ssr",
  "version": "0.8.1",
  "type": "module",
  "bin": {
    "vise": "bin/vise.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "types"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "clean:all": "rimraf ./dist ./bin",
    "build": "npm run clean:all && rollup -c ./rollup.config.js",
    "watch": "npm run clean:all && rollup -w -c ./rollup.config.js",
    "lint": "eslint --ext .ts,.js,.vue ./src"
  },
  "dependencies": {
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/html-minifier-terser": "^6.1.0",
    "@vise-ssr/shared": "0.8.1",
    "@vise-ssr/react": "0.8.1",
    "@vise-ssr/vue3": "0.8.1",
    "@vitejs/plugin-legacy": "^2.2.0",
    "chalk": "^5.0.0",
    "commander": "^8.1.0",
    "copyfiles": "^2.4.1",
    "enquirer": "^2.3.6",
    "esbuild": "^0.14.2",
    "esbuild-plugin-alias": "^0.2.0",
    "express": "^4.17.1",
    "glob": "^7.2.0",
    "html-minifier-terser": "^6.1.0",
    "minimist": "^1.2.5",
    "postcss": "^8.3.8",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "serialize-javascript": "^6.0.0",
    "tapable": "^2.2.1",
    "terser": "^5.15.0",
    "vite": "^3.1.4",
    "vitest": "^0.25.7",
    "zx": "^6.0.7"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/copyfiles": "^2.4.1",
    "@types/express": "^4.17.13",
    "@types/glob": "^8.0.0",
    "@types/rimraf": "^3.0.2",
    "@types/serialize-javascript": "^5.0.2",
    "dotenv": "^16.0.1",
    "rollup-plugin-executable": "^1.6.3",
    "rollup-plugin-preserve-shebangs": "^0.2.0",
    "@rollup/plugin-terser": "^0.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~4.6.3"
  },
  "peerDependenciesMeta": {
    "rollup": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14.8.0"
  },
  "keywords": [
    "vise",
    "ssr",
    "vite",
    "vue3"
  ],
  "description": "Vise 读音[vaɪs]，是一个同构 SSR 开发框架，致力于打造开箱即用的同构 Web 页面开发体验。通过插件化方式，支持任意服务端框架与任意前端框架的组合使用。使用基于 esm、速度更快的 vite 代替常见 Webpack 作为开发构建工具，提供命令行工具支持一站式的开发、构建、发布 Web 应用，让业务项目可以关注在业务功能实现上。项目基于全方位 ESM 及 TypeScript。",
  "homepage": "https://stauren.github.io/vise-ssr/",
  "bugs": {
