    "version": "0.2.0",
    "configurations": [
      {
        "name": "Start Vise Intro Dev",
        "preLaunchTask": "",
        "request": "launch",
        "cwd": "${workspaceFolder}/playground/app-vue3-intro",
        "command": "npm run dev",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "type": "node-terminal"
      },
      {
        "name": "Start Express Serve",
        "request": "launch",
        "cwd": "${workspaceFolder}/packages/express-server",
        "command": "npm run serve",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "type": "node-terminal"
      },
      {
        "name": "Start Vitest",
        "preLaunchTask": "",
        "request": "launch",
        "cwd": "${workspaceFolder}/",
        "command": "npm run test",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "type": "node-terminal"
      },
    ]
}